config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": "EnemyBase",
"language": "GDScript",
"path": "res://enemies/enemy_base.gd"
} ]
_global_script_class_icons={
"EnemyBase": ""
}

[application]

config/name="shmup-platformer"
run/main_scene="res://main.tscn"
boot_splash/use_filter=false
config/icon="res://icon.png"

[autoload]

BulletManager="*res://bullet_manager/bullet_manager.gd"
GameStateManager="*res://resources/game_state.gd"

[display]

window/size/width=960
window/size/height=540
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/AsepriteWizard/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

