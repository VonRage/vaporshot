[application]

config/name="shmup-platformer"
run/main_scene="res://main.tscn"
boot_splash/use_filter=false
config/icon="res://icon.png"

[autoload]

BulletManager="*res://bullet_manager/bullet_manager.gd"
GameStateManager="*res://resources/game_state.gd"
procam="*res://addons/ProCam2D/scripts/autoload.gd"
SaveSystem="*res://addons/save_system/save_system.gd"
Logger="*res://addons/gs_logger/logger.gd"

[display]

window/size/width=960
window/size/height=540
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/AsepriteWizard/plugin.cfg", "res://addons/ProCam2D/plugin.cfg", "res://addons/gs_logger/plugin.cfg", "res://addons/save_system/plugin.cfg", "res://addons/sticky_scribe/plugin.cfg", "res://addons/time-tracker/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

